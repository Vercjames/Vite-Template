    "start:prod": "yarn env:prod  && vite --host --port 8000",
    "start:stage": "yarn env:stage && vite --host --port 8000",
    "env": "node client-configs/envs/env.js VITE_BUILD=dev",
    "env:dev": "node client-configs/envs/env.js VITE_BUILD=dev",
    "env:prod": "node client-configs/envs/env.js VITE_BUILD=prod",
    "env:stage": "node client-configs/envs/env.js VITE_BUILD=stage",
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint:fix": "eslint --fix \"./client-apps/**/*.{js,jsx,ts,tsx,json}\"",
    "lint": "eslint \"./client-apps/**/*.{js,jsx,ts,tsx,json}\"",
    "preview": "vite preview",
    "build": "vite build"
  },
  "dependencies": {
    "@emotion/react": "^11.10.4",
    "@emotion/styled": "^11.10.4",
